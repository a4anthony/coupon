{
    "name": "a4anthony/coupon",
    "description": "E-commerce coupon generator and dashboard",
    "license": "MIT",
    "authors": [{
        "name": "a4anthony",
        "email": "anthonygakro@gmail.com"
    }],
    "minimum-stability": "dev",
